/* pages/y-myorderHouse/y-myorderHouse.wxss */
page{
  padding-bottom: 80rpx;
}
.header{
  position: fixed;
  top: 0;
  width: 100%;
  height: 86rpx;
  background: #FFF;
  border-bottom: 1px solid #ccc;
}
.empty-page image {
  width: 208rpx;
  height: 154rpx;
  margin-top: 300rpx;
  margin-bottom: 10rpx;
}

.empty-page {
  height: 100%;
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
  align-items: center;
  justify-content: center;
  color: #666;
}


.headerHeight{
  height: 86rpx;
}
.header>view{
  text-align: center;
  font-size: 30rpx;
  padding-bottom: 12rpx;
  border-bottom: 4rpx solid transparent;
}
.header> .active{
  color: #F35B1D;
  border-bottom: 4rpx solid  #F35B1D;
}
.list{
  background: #FFF;
  margin: 0 22rpx;
  padding: 52rpx 21rpx 49rpx;
  box-sizing: border-box;
}
.marginList{
  margin-top: 60rpx;
}
.makeRoomInformation{
  align-items: flex-start;
  margin-top: 30rpx;
}
.makeRoomInformation>image{
  width: 86rpx;
  height: 82rpx;
}
.houseName{
  margin-left: 22rpx;
  flex: 1;
}
.roomName{
  font-size: 34rpx;
}
.dates{
  color: #C4C5C7;
  font-size: 24rpx;
  margin-bottom: 24rpx;
  text-align: center;
}
.orderRoomDate{
  font-size: 24rpx;
  color: #C4C5C7;
  margin-top: 20rpx;
}
.type,.typeTeam{
  color: #F35B1D;
  font-size: 30rpx;
}

.orderMoney{
  margin-top: 36rpx;
  font-size: 24rpx;
  color: #C4C5C7;
}
.orderMoney text{
  font-size: 30rpx;
  color: #878689;
}
.orderMoney>view:first-of-type{
  margin-right: 38rpx;
}
.flex_jin{
  display: flex;
  align-items: center;
  justify-content: flex-end;
  margin-top: 30rpx;
}
.style_0 text{
  border:1px solid #C4C5C7;
  padding: 16rpx 32rpx;
  border-radius: 16rpx;
  color: #878689;
}
.style_1{
  color: #E60012;
}
.style_2{
  display: flex;
  align-items:center;
}
.style_2 view{
  padding: 16rpx;
  border: 1px solid #F35B1D;
  color: #F35B1D;
  border-radius: 16rpx;
}
.style_2 view:last-of-type{
  margin-left: 20rpx;
}