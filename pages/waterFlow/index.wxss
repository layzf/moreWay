

/*底部悬浮*/

.fiexedBottom {
  position: fixed;
  bottom: 0;
  background: rgba(250, 250, 250, 0.9);
  width: 100%;
  height: 128rpx;
  display: flex;
}

.fiexedBottom::after {
  position: absolute;
  bottom: 0;
  content: '';
  height: 56rpx;
  width: 2rpx;
  background: #2d2d2d;
  left: 50%;
  margin-left: -1rpx;
  top: 50%;
  margin-top: -28rpx;
}

.fiexedBottom>view {
  flex: 1;
  color: #1f1f1f;
  font-size: 32rpx;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
}

.fiexedBottom image{
  width: 36rpx;
  height: 36rpx;
  margin-right: 14rpx;
}
.fiexedBottom>view:last-of-type image{
  width: 40rpx;
}