@import "/tpls/auth-code/auth-code-tpl.wxss";
@import "/tpls/loginPublic/loginPublic.wxss";
page {
    height: 100%;
    color: #999;
    background-color: #F7F7F7;
}

.check{
    background-color: #ffffff!important;
    height:100rpx;
    width:100%;
    line-height:100rpx;
    text-align:center;
    color: #E94816;
    display: flex;
    align-items: center;
}

.agent{
    display: flex;
    justify-content: flex-start;
    height: 100%;
}
.agent .agent_slider_left{
    flex: 2;
    display: flex;
    flex-direction: column;
    font-size: 26rpx;
    align-items: center;
    color: #666;
    background-color: #f7f7f7;
}

.agent .agent_slider_left slider_text view{
     width:80%;
     margin:0 auto;
     height:60rpx;
     line-height:60rpx;
 }

.agent .agent_slider_left .check view{
    width:100%;
    margin:0 auto;
    height:50rpx;
    line-height:50rpx;
    border-left: 10rpx solid #E94816;
}

.agent .agent_slider_left .slider_tag{
    background-color: #020202;
    color: #FFFFFF;
    border-radius: 30rpx;
    padding: 6rpx 20rpx;
    margin-top: 40rpx;
}

.agent .agent_slider_left .slider_title{
    color: red;
    margin-top: 40rpx;
}
.agent .agent_slider_left .slider_text{
    height:100rpx;
    width:100%;
    line-height:100rpx;
    text-align:center;
}

.agent .agent_slider_right{
    flex:7;
    background-color: #ffffff;
    position: relative;
}
.agent .agent_slider_right .right_head{
    width:89%;
    height:130rpx;
    background-color:rgba(0,0,0,0.2);
    color:#000;
    margin:0 auto;
    line-height:100rpx;
    margin-top:40rpx;
    font-size:24rpx;
    text-align: center;
}

.agent .agent_slider_right .right_title{
    color: #000;
    font-size: 26rpx;
    padding-left: 28rpx;
    display: block;
    margin-top: 30rpx;
    font-weight: 600;
}

.agent .agent_slider_right .slider_right_content{
    display: flex;
    flex-wrap: wrap;
    justify-content: flex-start;
}

.agent .agent_slider_right .slider_right_content .slider_content{
    width: 27%;
    display: flex;
    align-items: center;
    flex-direction: column;
    margin-left: 28rpx;
    margin-top: 30rpx;
}

.agent .agent_slider_right .slider_right_content .slider_content image{
     width: 100%;
     height: 110rpx;
 }

.agent .agent_slider_right .slider_content text{
    font-size: 26rpx;
    padding-top: 6rpx;
}

.agent-item{
    display: flex;
    flex-direction: column;
    align-items: center;
    background-color: #f7f7f7;
}
.t0{
    display: flex;
    flex-direction: column;
    width: 94%;
    margin: 0 auto;
    padding-bottom: 40rpx;
}
.t0 .tline{
    font-size: 28rpx;
    color: #313131;
    padding-bottom: 20rpx;
    border-bottom: 1rpx solid rgba(180,180,180,0.2);
}
.t0 .tview{
    display: flex;
    justify-content: flex-start;
    flex-wrap: wrap;
    padding-bottom:20rpx;
}
.tview .titem{
    display: flex;
    flex-direction: column;
    align-items:center;
    margin-top:30rpx;
}
.titem view{
    color: #313131;
    font-size: 22rpx;
    width:160rpx;
    text-align:center;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
}
.titem image{
    width: 180rpx;
    height: 90rpx;
}
.agent-item .item{
    display: flex;
    flex-direction: column;
    padding-bottom: 32rpx;
    width: 94%;
    margin: 0 auto;
}
.item .i-data{
    display: flex;
    align-items: center;
}
.i-data .i-txt{
    color: #333333;
    padding-top: 32rpx;
    padding-bottom: 28rpx;
    font-size: 28rpx;
}
.i-data .i-label{
    text-align:center;
    border:2rpx solid #E94816;
    color:#E94816;
    font-size:20rpx;
    padding:2rpx 4rpx;
    margin-left:16rpx;
    -webkit-border-radius: 6rpx;
    -moz-border-radius: 6rpx;
    border-radius: 6rpx;
}
.item image{
    width: 100%;
    height: 192rpx;
    border-radius: 10rpx;
}
.bottomLine{
    width: 100%;
    border-top: 1rpx solid rgba(180,180,180,0.3);
    position: fixed;
    bottom: 0;
    left: 0;
}
.marginTop{
    padding-top: 40rpx;
}
.jusity{
    justify-content: flex-start !important;
}
.ibanner,.baseinfo>image{
    width: 94%;
    margin-left: 3%;
    height: 192rpx;
    margin-top: 20rpx;
    margin-bottom: 20rpx;
}
.baseinfo>image{
    margin: 0 0 0rpx 3%;
    border-radius: 10rpx;
}
.baseinfo view{
  padding-left: 20rpx;
}
.nonImg{
    position: absolute;
    right: 0;
    top: 10rpx;
    width: 100%;
    height: 192rpx;
    display:flex;
    justify-content:center;
}
.nonImg image{
    width: 94%;
    height: inherit;
    margin: 0 auto;
    border-radius: 10rpx;
}
