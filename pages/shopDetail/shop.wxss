}

.modal .modal_content .shop_list .shop_item .s_i_title .i_text{
    color: #9B9B9B;
    font-size: 26rpx;
}

.modal .modal_content .shop_list .shop_item .s_i_num{
    display: flex;
    align-items: center;
    flex: 2;
    justify-content: flex-end;
    padding-right: 20rpx;
}

.modal .modal_content .shop_list .shop_item .s_i_num image{
    width: 42rpx;
    height:42rpx;
    margin-left: 20rpx;
    margin-right: 20rpx;
}
.modal .modal_content .car_foot{
    position: absolute;
    background-color: #FFFFFF;
    bottom: 0;
    left: 0;
    width: 100%;
    display: flex;
    align-items: center;
    height: 100rpx;
    border-top: 1rpx solid rgba(180,180,180,0.3);
}
.modal .modal_content .car_foot .f_text{
