}
.modal{
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    background-color: rgba(0,0,0,0.8);
    height: 100%;
    z-index: 90;
}
.modal .modal_content{
    background-color: #FFFFFF;
    width: 100%;
    height: 60%;
    position: fixed;
    bottom: 0px;
    left: 0;
    z-index: 99;
    transition: transform 400ms linear 0s;
}
.modal .modal_content .clearCar{
    color: #999999;
    background-color:#F2F2F2;
    text-align: right;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 28rpx;
    padding-right: 40rpx;
    display: flex;
    align-items: center;
    justify-content: flex-end;
}
.modal .modal_content .clearCar image{
    width:34rpx;
    height:32rpx;
    padding-right:10rpx;
}
.modal .modal_content .shop_list{
    display: flex;
    flex-direction: column;
    background-color: #FFFFFF;
}

.modal .modal_content .shop_list .shop_item{
    display: flex;
    padding-top: 20rpx;
    padding-bottom: 20rpx;
    align-items: center;
    border-bottom: 1rpx solid rgba(180,180,180,0.2);
}

.modal .modal_content .shop_list .shop_item .s_i_title{
    display: flex;
    flex: 3;
    flex-direction: column;
    text-align: left;
    padding-left: 28rpx;
}
.modal .modal_content .shop_list .shop_item .s_i_title .i_title{
    color: #333333;
    font-weight: 600;
}
.modal .modal_content .shop_list .shop_item .s_i_title .i_txt{
