page{
  background: #FFF;
}
.seachBox{
  display: flex;
  align-items: center;
  width: 672rpx;
  margin: 30rpx auto 40rpx;
}
.seachBox .seachBtn{
  padding: 12rpx 20rpx;
  border: 1px solid #666;
  border-radius: 10rpx;
  margin-left: 20rpx;
}
.seachBox .searchView{
  display: flex;
  align-items: center;
  padding: 15rpx 0;
  border:1px solid #999999;
  border-radius: 8px;
  padding-left: 16rpx;
  flex: 1;
}
.seachBox .searchView input{
  margin-left: 16rpx;
  width: 100%;
}
.colseInt{
  margin-right:20rpx;
}

.hotVillage{
  width: 672rpx;
  margin: 0 auto;
  margin-top: 50rpx;
}
.villageList{
  display:flex;
  align-items: center;
  margin-top: 35rpx;
  flex-wrap: wrap;
  margin-bottom: 150rpx;
}
.villageList>view{
  margin-right:20rpx;
  border: 1px solid #F19216;
  color: #F19216;
  border-radius: 10rpx;
  padding: 8rpx 20rpx; 
  text-align: center;
  margin-bottom: 30rpx;
}

.villageList view{
  color: #F19216;
}

.villageList .num{
  font-size: 28rpx;
  padding: 0;
  line-height: normal;
  color: #F19216;
}
.hotvillagename{
  text-align: center;
  width: 672rpx;
  margin: 0 auto;
  font-size: 28rpx;
  color: #F19216;
}

.hotvillagename text{
  text-decoration: underline;
}

.hotvillagename .num{
  padding: 10rpx 0;
  color: #F19216;
  line-height: normal;
  font-size: 28rpx;
}

.absoluteBotm{
  position: absolute;
  bottom: 100rpx;
  width: 100%;
  height: auto;
}