.online{
    background-color: #F7F7F7;
}
.headstep{
    height: 97rpx;
}
.head{
    display: flex;
    align-items: center;
    padding-top: 16rpx;
    background-color: #f7f7f7;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    z-index: 60;
    border-bottom: 1rpx solid rgba(180,180,180,0.2);
}
.head .h_left{
    background-color: #D8D8D8;
    height: 64rpx;
    border-radius: 32rpx;
    width: 474rpx;
    display: flex;
    align-items: center;
    margin-left: 28rpx;
    margin-bottom: 12rpx;
}
.h_left image{
    width: 34rpx;
    height: 34rpx;
    margin-left: 20rpx;
}
.h_left .searchInput{
    flex: 4;
    padding-left: 10rpx;
}
.head .h_img{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    margin-left: 48rpx;
    padding-bottom: 12rpx;
}
.h_img image{
    width: 16px;
    height: 19px;
}
.h_img text{
    color: #9B9B9B;
    font-size: 20rpx;
    padding-top:8rpx
}
.banner{
    display: flex;
    flex-direction: column;
}
.banner image{
    width: 100%;
}
.content{

}
.item{
    display: flex;
    justify-content: space-between;
    align-items: center;
    background-color: #ffffff;
    padding-left: 20rpx;
    padding-right: 20rpx;
    margin-top: 20rpx;
    padding-top: 20rpx;
}
.item .i_txt{
    border-left: 6rpx solid red;
    padding-left:8rpx
}
.item .i_more{
    font-size: 26rpx;
    color: #7D7D7D;
    display: flex;
    align-items: center;
}
.i_more image{
    width:18rpx;
    height:26rpx;
    padding-left:10rpx;
}
.content .c_list{
    display: flex;
    flex-wrap: wrap;
    background-color: #ffffff;
    padding: 20rpx 26rpx;
    justify-content: space-between;
}
.c_list .more{
    color: #333333;
    height: 58rpx;
    text-align: center;
    width: 100%;
    font-size: 28rpx;
    border-radius: 10rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    padding-top: 26rpx;
    padding-bottom: 26rpx;
    line-height:58rpx;
}
.more image{
    width:6px;
    height:10px;
    margin-left:16rpx;
}
.c_list .c_li{
    width: 48%;
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-bottom: 44rpx;
}
.c_li .li_img{
    flex:2;
    /*border:1rpx solid rgba(180,180,180,0.2);*/
    /*box-shadow:0 0 20rpx rgba(180,180,180,0.3);*/
    width:100%;
    border-radius:20rpx;
}
.li_img .li_foot{
    display: flex;
    justify-content: space-between;
    padding-top: 10rpx;
}
.li_foot .symbol{
    display: flex;
    align-items: flex-end;
    color: red;
}
.li_foot .shopCar{
    width: 32rpx;
    height: 36rpx;
}
.shopCar .carimg{
    width: inherit;
    height: inherit;
    border: none;
}
.symbol .s1{
    font-size: 22rpx;
}
.symbol .s2{
    font-size: 28rpx;
}
.del{
    font-size:24rpx;
    text-decoration:line-through;
    font-style:italic;
    color: #999999;
}
.li_foot .li-price{
    display: flex;
    align-items: center;

}
.li_img image{
    width: 100%;
    border: 1rpx solid rgba(180,180,180,0.2);
    border-radius: 10rpx;
}
.c_li .li_text{
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    flex: 4;
    height:210rpx;
    padding-left: 20rpx;
}
.li_text .title{
    color: #333333;
    font-size: 28rpx;
}
.li_text .contxt{
    color:#999999;
    font-size: 26rpx;
}
.li_text .l_btn{
    color: #3979D4;
    font-size: 28rpx;
}
.foot{
    display: flex;
    align-items: center;
    height: 90rpx;
    background-color: #ffffff;
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
}
.foot .fimg{
    width: 80rpx;
    height: 80rpx;
    margin-top:-48rpx;
    margin-left:28rpx;
    position: relative;
}
.fimg image{
    width: 80rpx;
    height: 80rpx;
}
.fimg .carNum{
    color:#ffffff;
    background-color:#E63E36;
    border:1px solid #ffffff;
    -webkit-border-radius:20px;
    -moz-border-radius:20px;
    border-radius:20px;
    display:inline-block;
    padding:-1rpx;
    position:absolute;
    right:10rpx;
    top:10rpx;
    font-size:20rpx;
    height:10px;
    width:10px;
    line-height:10px;
    text-align:center;
}
.fprice{
    display: flex;
    flex-direction: column;
    align-items: flex-start;
    padding-left: 14rpx;
    flex: 4;
}
.fprice .price{
    color: #E94816;
    font-size: 36rpx;
    font-weight: 600;
}
.fprice .f_txt{
    color: #9B9B9B;
    font-size: 20rpx;
    padding-bottom:10rpx;
    padding-left:4rpx;
}
.f_txt .f_sub{
    padding-left: 28rpx;
}
.fbtn{
    color: #ffffff;
    font-size: 30rpx;
    background-color: #E94816;
    height: 90rpx;
    width: 210rpx;
    text-align: center;
    line-height: 90rpx;
}
.modal{
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    background-color: rgba(0,0,0,0.8);
    height: 100%;
    z-index: 90;
}
.modal .modal_content{
    background-color: #FFFFFF;
    width: 100%;
    height: 60%;
    position: absolute;
    bottom: 0px;
    left: 0;
    z-index: 99;
    transition: transform 400ms linear 0s;
}
.modal .modal_content .clearCar{
    color: #999999;
    background-color:#F2F2F2;
    text-align: right;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 28rpx;
    padding-right: 40rpx;
    display: flex;
    align-items: center;
    justify-content: flex-end;
}
.modal .modal_content .clearCar image{
    width:34rpx;
    height:32rpx;
    padding-right:10rpx;
}
.modal .modal_content .shop_list{
    display: flex;
    flex-direction: column;
    background-color: #FFFFFF;
}

.modal .modal_content .shop_list .shop_item{
    display: flex;
    padding-top: 20rpx;
    padding-bottom: 20rpx;
    align-items: center;
    border-bottom: 1rpx solid rgba(180,180,180,0.2);
}

.modal .modal_content .shop_list .shop_item .s_i_title{
    display: flex;
    flex: 3;
    flex-direction: column;
    text-align: left;
    padding-left: 28rpx;
}
.modal .modal_content .shop_list .shop_item .s_i_title .i_title{
    color: #333333;
    font-weight: 600;
}
.modal .modal_content .shop_list .shop_item .s_i_title .i_txt{
    color: #9B9B9B;
    font-size: 20rpx;
    padding-top: 6rpx;
}
.modal .modal_content .shop_list .shop_item .s_i_price{
    color:#E63E36;
    flex:2;
    text-align:right;
    font-weight:600;
    padding-right: 20rpx;
    font-size:32rpx;
}

.modal .modal_content .shop_list .shop_item .s_i_title .i_text{
    color: #9B9B9B;
    font-size: 26rpx;
}

.modal .modal_content .shop_list .shop_item .s_i_num{
    display: flex;
    align-items: center;
    flex: 2;
    justify-content: flex-end;
    padding-right: 20rpx;
}

.modal .modal_content .shop_list .shop_item .s_i_num image{
    width: 42rpx;
    height:42rpx;
    margin-left: 20rpx;
    margin-right: 20rpx;
}
.modal .modal_content .car_foot{
    position: absolute;
    background-color: #FFFFFF;
    bottom: 0;
    left: 0;
    width: 100%;
    display: flex;
    align-items: center;
    height: 100rpx;
    border-top: 1rpx solid rgba(180,180,180,0.3);
}
.modal .modal_content .car_foot .f_text{
    color: #333333;
    font-size: 30rpx;
    font-weight: 600;
    padding-left: 40rpx;
}
.modal .modal_content .car_foot .f_price{
    color:#E94816;
    font-size:32rpx;
    font-weight:600;
    flex:4;
    display:flex;
    flex-direction:column;
    height:80rpx;
    padding-left:10rpx;
    padding-top:10rpx;
    justify-content: center;
}

.modal .modal_content .car_foot .fbtn{
    background-color: #E94816;
    color: #FFFFFF;
    height: 100rpx;
    text-align: center;
    flex: 2;
    font-size: 32rpx;
    line-height: 100rpx;
}

.modal .modal_content .modal_head{
    display: flex;
}
.modal .modal_content .modal_head .m_head_img{
    width:200rpx;
    height:200rpx;
    border:1px solid #999999;
    margin-right:20rpx;
    margin-left:80rpx;
    margin-top:-66rpx;
    background-color:#999;
    border-radius:10rpx;
}

.modal .modal_content .modal_head .m_head_title{
    color: #333333;
    padding-left: 10rpx;
    padding-top: 30rpx;
    font-size: 28rpx;
    font-weight: 600;
}
.modal .modal_content .m_content_color{
    display: flex;
    flex-direction: column;
    margin-top: 54rpx;
}
.modal .modal_content .m_content_color .m_head_title{
    color: #333333;
    font-size: 28rpx;
    margin-left: 80rpx;
}
.modal .modal_content .m_content_color .m_content_choose{
    display: flex;
    margin-top: 16rpx;
}

.modal .modal_content .m_content_num{
    display: flex;
    align-items: center;
    margin-top: 54rpx;
}

.modal .modal_content .m_content_num .m_head_title{
    margin-left: 80rpx;
    color: #333333;
}
.modal .modal_content .m_content_num  image{
    width: 42rpx;
    height:42rpx;
    margin-left: 20rpx;
    margin-right: 20rpx;
}
.addToCar{
    background-color: #E94816;
    color: #FFFFFF;
    font-size: 34rpx;
    font-weight: 600;
    width: 100%;
    position: absolute;
    bottom: 0;
    left: 0;
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
}
.noMore{
    display: flex;
    justify-content: space-between;
    color: #C3B99D;
    font-size: 28rpx;
    align-items: center;
}
.noMore .n-line{
    width: 40%;
}
.returnImg{
    width: 100rpx;
    height:100rpx;
    -webkit-border-radius: 100rpx;
    -moz-border-radius: 100rpx;
    border-radius: 100rpx;
    border: 6rpx solid yellow;
}
.step{
    height: 90rpx;
}
.title {
    display: flex;
    align-items: center;
    height: 30rpx;
}
.title .tline{
    background-color: #ff5d22;
    width: 8rpx;
    height: 30rpx;
    -webkit-border-radius: 10rpx;
    -moz-border-radius: 10rpx;
    border-radius: 10rpx;
}
.title .ttxt{
    padding-left: 10rpx;
    font-size: 32rpx;
    color: #333;
    font-weight: 600;
}
.contact{
    padding-left:18rpx;

    padding-right: 0;
}
.contactView text {
    font-size:20rpx;
    color:#9B9B9B;
    padding-top:6rpx;
}
button{
    background-color: #f7f7f7 !important;
}
