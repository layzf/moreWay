.success{
    background-color: #F7F7F7;
}
.head{
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}
.head .himg{
    margin-top: 76rpx;
    width: 186rpx;
    height: 186rpx;
}
.head text{
    color: #000000;
    font-size: 40rpx;
    margin-top: 28rpx;
}
.content{
    color: #888888;
    width: 90%;
    margin: 30rpx auto 50rpx auto;
    display: flex;
    flex-direction: column;
    text-align: center;
}
.btn{
    width: 90%;
    height:94rpx;
    line-height: 94rpx;
    margin: 0 auto;
    text-align: center;
    border-radius: 20rpx;
    font-size: 36rpx;
}
.red{
    background-color: #E94816;
    color: #ffffff;
}
.white{
    background-color: #ffffff;
    border: 1rpx solid rgba(5,5,5,0.1);
    color: #000000;
    margin-top: 30rpx;
}
