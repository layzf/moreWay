.form-list{
  width: 100%;
  height: 110rpx;
  background: #ffffff;
  margin: 20rpx 0 0 0;
}
.form-con{
  width: 694rpx;
  height: 109rpx;
  border-bottom: 1rpx solid #E9E9E9;
  display: flex;
  flex-flow: nowrap;
  align-items: center;
  margin: 0 auto;
}
.img{
  width: 56rpx;
  height: 56rpx; 
}
.form-list .form-con .title{
  font-size: 28rpx;
  color: #666666;
  margin-left:16rpx; 
}
.statistics{
  width: 100%;
  height: 174rpx;
  background: #ffffff;
  display: flex;
}
.statistics .tit-box{
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.statistics .tit{
  font-size: 24rpx;
  color: #999999;
}
.statistics .num{
  font-size: 40rpx;
  color: #3979D4;
  margin: 30rpx 0 0 0;
}
.statistics .line{
  width: 2rpx;
  height: 80rpx;
 background: #E9E9E9;
 margin: 55rpx 0 0 0;
}
.detail_title{
  width: 100%;
  height: 90rpx;
  background: #ffffff;
  margin: 20rpx 0 0 0;
  font-size: 30rpx;
  color: #333333;
   display: flex;
 flex-flow: nowrap;
 align-items: center;
 padding: 0 0 0 28rpx;
 border-bottom: 1rpx solid #E9E9E9;
}
.time{
  width: 100%;
  background: #ffffff;
  height: 48rpx;
  display: flex;
  flex-flow: nowrap;
  align-items: flex-end;
  padding: 0rpx 0 0 28rpx;
  color: #999999;
  font-size: 28rpx;
}
.list{
   width: 100%;
  height: 108rpx;
  background: #ffffff;
}
.list .content{
  width: 694rpx;
  height: 107rpx;
  border-bottom: 1rpx solid #E9E9E9;
  margin: 0 auto;
  display: flex;
  flex-flow: nowrap;
  align-items: center;
  justify-content: space-between;
}
.name{
  font-size: 28rpx;
  color: #333333;
}
.stuat{
  font-size: 28rpx;
  color: #FF5D22;
}