.head{
  width: 100%;
  height: 108rpx;
  background: #ffffff;
  margin: 10rpx 0 0 0;
  display: flex;
  justify-content: space-between;
  flex-flow: nowrap;
  align-items: center;
}
.head .nickname{
    display: flex;
  flex-flow: nowrap;
  margin: 0 0 0 56rpx;
}
.head .nickname .nickname_p{
  margin: 0 0 0 40rpx;
}
.head .button{
  width: 118rpx;
  height: 50rpx;
  font-size: 26rpx;
  background:rgba(233,72,22,0.1);
  border:1px solid rgba(233,72,22,0.5);
  border-radius: 8rpx;
  color: #E94816;
  line-height: 50rpx;
  text-align: center;
  margin: 0 68rpx 0 0;
}
.content{
  width: 100%;
  background: #ffffff;
  margin: 10rpx 0 0 0;
  display: block;
  overflow: hidden;
}
.content .address{
  margin: 40rpx 0 0 0;
  height: 80rpx;
  display: flex;
  flex-flow: nowrap;
  align-items: center;
  justify-content: space-around;
}
.content .address .box{
  display: flex;
  flex-flow: nowrap;
  align-items: center;
}
.content .village{
    height: 108rpx;
    display: flex;
  flex-flow: nowrap;
  margin: 10 10 10 56rpx;
}
.content .village .title{
  color: #999999;
  font-size: 28rpx;
  margin: 0rpx 0 0 40rpx;
  flex-flow: nowrap;
}

.content .village .village_name{
  display: flex;
  flex-flow: nowrap;
  margin-top: 40rpx;
  margin-left: 40rpx;
  float: left;
}

.content .address input{
  width: 138rpx;
  height: 80rpx;
  border: 1rpx solid #E9E9E9;
}
.prove{
  width: 100%;
  height: 200rpx;
  display: flex;
  flex-flow: nowrap;
  margin: 54rpx 0 0 0rpx;
}
.prove .title{
  color: #999999;
  font-size: 28rpx;
  margin: 0rpx 0 0 40rpx;
}
.prove .img{
  width: 198rpx;
  height: 200rpx;
  margin: 0rpx 0 0 40rpx;
}
.prove .img1{
  width: 240rpx;
  height: 160rpx;
  margin: 0rpx 0 0 40rpx;
}
.foot{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  display: flex;
}
.foot .left{
  flex: 1;
  background:rgba(51,51,51,1);
  font-size: 30rpx;
  color: #ffffff;
  text-align: center;
  line-height: 100rpx;
}
.foot .right{
  flex: 1;
  background:rgba(255,93,34,1);
  font-size: 30rpx;
  color: #ffffff;
  text-align: center;
  line-height: 100rpx;
}

/* //弹出层 */
.bg{
  width: 100%;
  height:100%;
  position: fixed;
  z-index: 99;
  background: #000000;
  opacity: 0.4;
  top:0
}
.model{
  width: 630rpx;
  height: 490rpx;
  background: #ffffff;
  position: absolute;
  top:50%;
  left:50%;
  transform:translate(-50%,-50%);
  z-index: 999;
}
.model .title{
  font-size: 32rpx;
  color: #333333;
  text-align: center;
  margin: 40rpx;
}
.model .textarea{
  width: 550rpx;
  height: 240rpx;
  background: #E9E9E9;
  margin: 0 0 0 40rpx;
}
.model button{
  width: 240rpx;
  height: 68rpx;
  background: #FF5D22;
  border-radius: 4rpx;
  margin: 30rpx 0 0 196rpx;
  color: #ffffff;
  font-size: 28rpx;
}
.villageBig{
  width: 100%
}
.village_input{
  margin:0rpx 0 0 40rpx;
  border-bottom: 1px solid #F2F0F2;
}
.village_input input{
  color: #DBDBDB;
  padding-top: 10px;
}