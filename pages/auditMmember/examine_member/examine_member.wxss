.form_l{
  width: 100%;
  height: 134rpx;
  background: #ffffff;
  margin: 20rpx 0 0 0;
}
.box{
  width: 694rpx;
  height: 133rpx;
  border-bottom: 1rpx solid #E9E9E9;
  display: flex;
  flex-flow: nowrap;
  align-items: center;
  justify-content: space-between;
}
.box .info{
  display: flex;
  flex-flow: nowrap;
  align-items: center;
}
.box .img{
  width: 70rpx;
  height: 70rpx;
    margin: 0 0 0 28rpx;
}
.info .detail_info{
   display: flex;
  flex-direction: column;
  margin: 0 0 0 30rpx;
}
.box .button{
  width: 118rpx;
  height: 50rpx;
  background:rgba(233,72,22,0.1);
  border:1px solid rgba(233,72,22,0.5);
  border-radius: 8rpx;
  color: #E94816;
  line-height: 50rpx;
  text-align: center;
}
.title{
  font-size: 30rpx;
  color: #666666;
  margin: 30rpx 0 0 28rpx
}
.form_record{
  width: 100%;
  height: 134rpx;
  background: #ffffff;
  margin: 30rpx 0 0 0;
}
.stuat{
    display: flex;
  flex-direction: column;
  align-items: flex-end;
}
.stuat .date{
  color: #999999;
  font-size: 28rpx;
}