.success{
    background-color: #f4f4f4;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    text-align: center;
}
.success .head{
    width: 93px;
    height: 93px;
    margin-top: 38px;
    margin-bottom: 14px;
}
.success .h_tip{
    color: #000;
    font-size: 20px;
}
.success .content{
    display: flex;
    flex-direction: column;
    justify-content: center;
    margin-top: 30px;
}
.success .content .c_title{
    font-size: 36rpx;
    color:#888888;
    text-align: left;
    width: 80%;
    margin: 0 auto;
    padding-bottom: 20rpx;
}
.success .content .c_word{
    color: #888888;
    font-size: 30rpx;
    text-align: left;
    padding-top: 1rpx;
    width: 80%;
    margin: 0 auto;
}
.success .h_num{
    font-size: 14px;
    color: #888888;
    padding-top: 14px;
    padding-bottom: 10px;
}
.success .btn{
    background-color:#E94816;
    width: 90%;
    height: 47px;
    text-align: center;
    line-height: 47px;
    color: #FFFFFF;
    -webkit-border-radius: 20rpx;
    -moz-border-radius: 20rpx;
    border-radius: 20rpx;
    font-size: 36rpx;
    margin-top: 20px;
}
.success .footer{
    margin-top: 254rpx;
}
.success .footer .f_tip{
    color: #576B95;
}
