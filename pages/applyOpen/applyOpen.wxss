page{
  height: 100%;
  font-size: 14px;
}
.container{
  width: 100%;
  height: 100%;
  position: relative;
}
.container image{
  width: 100%;
  height: 100%;
}
.container_left{
  width: 240px;
  line-height: 40px;
  position: absolute;
  bottom: 100px;
  left: 18%;
  background:#fef48b;
  border-radius:20px;
  color: #832515;
  font-size: 14px;
}
.container_right{
  bottom: 45px;
  left: 18%;
  background:#fef48b;
  color: #832515;
}
.container_top{
  width: 42px;
  height: 22px;
  position: absolute;
  left: 5%;
  top: 13px;
}
@media only screen and (max-width: 320px) {
.container_left{
  width: 240px;
  line-height: 40px;
  position: absolute;
  bottom: 100px;
  left: 13%;
  background:#fef48b;
  border-radius:20px;
  color: #832515;
  font-size: 14px;
}
.container_right{
  bottom: 45px;
  left: 13%;
  background:#fef48b;
  color: #832515;
}
}