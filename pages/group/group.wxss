@import "/tpls/auth-code/auth-code-tpl.wxss";
@import "/tpls/loginPublic/loginPublic.wxss";

.transForm{
  height: 600px;
  width: 100%;
  background: #cccccc;
}

/*安居佳*/
.bannersan{
  width: 89%;
  margin: 0 40rpx;
}

page {
  height: 100%;
  color: #999;
  background-color: #f7f7f7;
}
.topLine{
  width: 100%;
  border-top: 1rpx solid #9a9a9a;
  position: fixed;
  top: 0;
  left: 0;
}
.bottomLine{
  width: 100%;
  border-top: 1rpx solid rgba(180,180,180,0.3);
  position: fixed;
  bottom: 0;
  left: 0;
}
.header {
  height: 92rpx;
}

.estate .icon-location{
  width:26rpx;
  height:32rpx;
}

.estate .icon-arrow-down{
  width:18rpx;
  height:12rpx;
}

.estate text{
  margin:0 10rpx;
  color:#666;
}
.code {
  color:#333;
}
.code .auth-code{
  margin-bottom:4rpx;
}
.prositions{
  position: relative
}
 .postionImgs{
  position: fixed;
  width: 130rpx;
  height: 130rpx;
}
.lunbo{
  width: 100%;
  height: 320rpx;
}
.lunbo .slider-item {
  height: 100%;
  width: 100%;
}

.lunbo image {
  width: 100%;
  height: 100%;
}
.num-board{
  height:174rpx;
  padding:40rpx 26rpx 40rpx;
  box-sizing: border-box;
  background-color: #fff;
}
.num-board .summary{
  font-size:24rpx;
  color:#999;
}
.num-board .num{
  font-size:40rpx;
  color:#3979D4;
}
.num-board .border{
  height:70rpx;
  width:2rpx;
  background-color: #f7f7f7;
}
.collects-header {
  height: 32rpx;
  font-size: 26rpx;
  padding-top: 40rpx;
  width: 94%;
  margin: 20rpx auto 0 auto;
  background-color: #fff;
}
 .title {
  display: flex;
  align-items: center;
  height: 50rpx;
   line-height: 50rpx;
}
.title .tline{
  background-color: #ff5d22;
  width: 8rpx;
  height: 30rpx;
  border-radius: 10rpx;
}
.title .ttxt{
  padding-left: 10rpx;
  font-size: 32rpx;
  color: #313131;
  font-weight: 600;
  height: 50rpx;
  line-height: 50rpx;
}
.collects-header text{
  color: #7D7D7D;
  font-size: 24rpx;
}

.collect{
  background-color: #fff;
  padding-bottom: 10px;
}

.collect .cover{
  width:100%;
}

.container{
  padding:30rpx 24rpx;
}

.container .top{
  padding-bottom:20rpx;
}

.container .title{
  flex: 1;
  color: #020202;
  font-size:32rpx;
  /* height:36rpx; */
  overflow: hidden;
}

.container .apply{
  flex: 0 0 260rpx;
  text-align: right;
  color: #666;
  font-size:26rpx;
  height:36rpx;
  overflow: hidden;
}

.container .info{
  font-size:26rpx;
  color:#999;
}
.container .info .date{
  float:left;
  height:48rpx;
  color:#FF5D22;
}
.container .info .label{
  text-indent:24rpx;
}

.container .payType{
  display: inline-block;
  background-color: #F6F6F6;
  color:#aaa;
  border-radius: 5px;
  padding:4rpx 14rpx;
  margin-top:30rpx;
}
.empty{
  width: 100%;
  font-size:24rpx;
  color:#999;
  text-align: center;
  height:104rpx;
  line-height:104rpx;
}

.containers{
  width: 100%;
  height: 100%;
  position: relative;
}
.containers image{
  width: 100%;
  height: 100%;
}
.progress {
  width: 100%;
  text-align: center;
  position: absolute;
  top: 20px;
  left: 0;
  color: #0a5ef5;
  font-weight: 600;
  font-size: 18px;
}
.singUp{
  width: 100%;
  text-align: center;
  position: absolute;
  bottom: 25%;
  color: #0a5ef5;
  font-size: 19px;
  font-weight: 600;
}
.progessBar{
  width: 70%;
  position: absolute;
  bottom: 20%;
  left: 15%;
}
 .progessBar progress{
  width: 100%;
}
.comeTrue{
  width: 100%;
  position: absolute;
  text-align: center;
  bottom: 15%;
  font-size: 16px;
  color: #333333;
}
.comeTrue text{
  font-size: 18px;
  color: #ff8212;
}
.comeTrue_left{
  width: 230px;
  line-height: 35px;
  text-align: center;
  background: #FFA94F;
  position: absolute;
  bottom: 5%;
  left: 20%;
  color: #ffffff;
  border-radius: 16px;
}
@media only screen and (max-width: 320px) {
	body {
		font-size: 12px;
	}
  .comeTrue_left{
  width: 230px;
  line-height: 35px;
  text-align: center;
  background: #FFA94F;
  position: absolute;
  bottom: 5%;
  left: 13%;
  color: #ffffff;
  border-radius: 16px;
}
}
.headeres{
  position: absolute;
  top: 0;
  left: 3px;
  height:92rpx;
  background: #5DAFF8;
}
.estate .icon-location{
  width:26rpx;
  height:32rpx;
}
.estate .icon-arrow-down{
  width:18rpx;
  height:12rpx;
}
.estate text{
  margin:0 10rpx;
}
.limitTime{
  width: 100%;
  background: #ffffff;
  margin-bottom: 20rpx;
  padding-bottom: 20rpx;
  flex-direction: column;
}
.collectLunbo{
  width: 90%;
  height: 190px;
}
.collectLunbo .slider-item{
  width: 100%;
  height: 100%;
}
.collectLunbo image{
  width: 100%;
  height: 100%;
  border-radius: 10px;
}

/*用来包裹所有的小圆点  */
.dots{
  width: 190rpx;
  height: 30rpx;
  display: flex;
  flex-direction: row;
  position: absolute;
  left: 308rpx;
  bottom: -80rpx;
}
/*未选中时的小圆点样式 */
.dot{
  width: 40rpx;
  height: 13rpx;
  margin-right: 26rpx;
  background-color: #CCC9C4;
}
/*选中以后的小圆点样式  */
.active{
  width: 40rpx;
  height: 13rpx;
  background-color: #FF5D22;
}
/*拟态窗*/
.modal{
  background-color: rgba(0,0,0,0.8);
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  overflow: hidden;
}
.modal .m_img{
  width: 70%;
}
.modal .close{
  width:60rpx;
  height:60rpx;
  position:absolute;
  right:8%;
  top:20%;
}
.img_item{
  display: flex;
  flex-direction: column;
  width: 94%;
  margin-top: 40rpx;
  background: #F9F9F9;
  border-bottom-left-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
}
.img_item .img{
  width: 100%;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
}
.img_item .img_txt{
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  padding: 40rpx 20rpx 0rpx 20rpx;
  box-sizing: border-box;
}
.txt1{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.txt1 .t-type{
  color: #313131;
  font-size: 28rpx;
  font-weight: 600;
}
.txt1 .date{
  font-size: 24rpx;
  color: #E94615;
  display: flex;
  align-items: center;
  height:30rpx;
  line-height:30rpx;
  margin-top: 8rpx;
}
.date .red{ color: red}
.txt2{
  display: flex;
  justify-content: space-between;
  padding-top: 25rpx;
  padding-bottom: 35rpx;
  align-items: center;
}
.txt2 .t-left{
  display: flex;
  align-items: center;
}
.txt2 .tbtn{
  display: inline-block;
  padding: 10rpx 40rpx;
  background-color: #E94615;
  -webkit-border-radius: 40rpx;
  -moz-border-radius: 40rpx;
  border-radius: 40rpx;
  color: #ffffff;
}
.t-left image{
  width: 35rpx;
  height: 35rpx;
  -webkit-border-radius: 35rpx;
  -moz-border-radius: 35rpx;
  border-radius: 35rpx;
}
.t-left .bg1{
  width: 35rpx;
  height: 35rpx;
  border-radius: 35rpx;
  background-color: #F29B76;
}
.t-left text{
  padding-left: 7rpx;
}
.t-left .bg2{
  width: 35rpx;
  height: 35rpx;
  border-radius: 35rpx;
  /*background-color: #F6B37F;*/
  margin-left: -8rpx;
  box-shadow: 0 0 2rpx #fff;
}
.t-left .bg3{
  width: 35rpx;
  height: 35rpx;
  border-radius: 35rpx;
  /*background-color: #FACD89;*/
  margin-left: -8rpx;
  box-shadow: 0 0 2rpx #fff;
}
.t-left .bg4{
  width: 35rpx;
  height: 35rpx;
  border-radius: 35rpx;
  margin-left: -8rpx;
}
.g-item{
  display: flex;
  flex-direction: column;
  background-color: #ffffff;
  margin-top: 20rpx;
  padding-top: 34rpx;
}
.g-item .g-template{
  display: flex;
  flex-direction: column;
}
.g-temp{
  display: flex;
  width:94%;
  padding: 25rpx 0;
  margin:0 auto;
}
.g-temp .g-img{
  flex: 1;
  padding-top:6rpx;
}
.g-temp .g-btn{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: flex-end;
  font-size: 22rpx;
  flex: 1;
  padding:20rpx 0;
}
.g-btn .tip{
  color: #7D7D7D;
  padding-top:6rpx
}
.g-btn .btn{
  color: #ffffff;
  background-color: #E94615;
  display: inline-block;
  padding: 8rpx 40rpx;
  -webkit-border-radius: 24rpx;
  -moz-border-radius: 24rpx;
  border-radius: 24rpx;
}
.g-img image{
  width: 230rpx;
  height: 136rpx;
  border: 1rpx solid rgba(180,180,180,0.2);
}
.g-temp .g-name{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items:flex-start;
  flex:1;
  padding:20rpx 30rpx;
}
.g-name .gtitle{
  color: #434343;
  font-size: 28rpx;
}
.g-name .time{
  display: flex;
  flex-direction: column;
  font-size: 22rpx;
}
.time .t-title{
  color: #7D7D7D;
}
.time .red{
  color: #E94615;
}
.g-item .g-head{
  display: flex;
  justify-content: space-between;
  width: 94%;
  margin: 0 auto;
}
.g-head .g-title{
  font-size: 32rpx;
  color: #000;
  border-left: 10rpx solid #E94816;
  text-indent: 10rpx;
  font-weight: 600;
}
.g-head .g-more{
  display: flex;
  align-items: center;
  font-size: 24rpx;
}

.g-item .g-text{
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  padding-top: 18rpx;
  margin: 0 16rpx;
  padding-bottom:30rpx;
}
.g-text .g-li{
  width: 31%;
  height: 290rpx;
  border-right: 20rpx solid #fff;
}
.g-li image{
  width: 100%;
  height: inherit;
}
.g-text .g-li:nth-of-type(3n){
  border-right: 0rpx;
}

.g-text .g-li:nth-of-type(4),.g-text .g-li:nth-of-type(5),.g-text .g-li:nth-of-type(6){
  margin-top: -20rpx;
}

.bottom-line{
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: #999999;
  font-size: 20rpx;
}
.flex-head{
  display: flex;
  background-color: #ffffff;
  padding: 40rpx 0rpx;
  justify-content: space-between;
}
.flex-head .f-head{
  width:136rpx;
  height:30rpx;
  background:linear-gradient(90deg,rgba(254,166,7,1) 0%,rgba(233,70,21,1) 100%);
  border-top-left-radius:15px;
  border-bottom-left-radius:15px;
  text-align: center;
  line-height: 30rpx;
  font-size: 22rpx;
  color: #ffffff;
  padding: 6rpx 8rpx;
}
.f-head .f-txt{
  color: #333333;
  font-size: 32rpx;
  font-weight: 600;
}
.f-head .f-tip{
  color: #999999;
  font-size: 24rpx;
}
.flex-con{
  display: flex;
  justify-content: space-between;
}
.flex-con .f-item{
  display: flex;
  align-items: center;
  justify-content: center;
  margin-left:30rpx;
}
.f-head .f-img{
  width: 136rpx;
  height: 30rpx;
  border: 1rpx solid;
}
.f-item image{
  width:22rpx;
  height:24rpx;
}
.f-item text{
  color: #666666;
  font-size: 22rpx;
  padding-left:6rpx;
}

.ctip{
  color: #999999;
  padding: 0 40rpx;
  font-size: 28rpx;
  display: block;
  background-color: #ffffff;
}
.borderTop{
  border-top: 1rpx solid #f7f7f7;
}
.borderRight{
  border-right: 1rpx solid rgba(191,191,191,0.4);
}
.step{
  background-color: #f7f7f7;
  height: 74rpx;
  display: flex;
  align-items: center;
  padding-left: 3%;
}
.step .tip{
  display:inline-block;
  background-color:#FF3C00;
  color:#ffffff;
  font-size:24rpx;
  padding: 4rpx 14rpx;
  border-radius: 10rpx;
}
.step .txt{
  color: #313131;
  margin-left: 6rpx;
}
.step .end{
  margin-left: 20rpx;
  color: #313131;
}
.end .red{
  color: #E94615;
}
.slide-swiper{
  width: 100%;
  height: 50rpx;
}
.slide-image{
  width: 100%;
  height: 50rpx;
  border: 1rpx solid #fff;
}
.over2{
  background-color: gainsboro !important;
}
.bgSlide{
  border-left: 6rpx solid #fff;
}

/*自助报价*/
.quote{
  width: 94vw;
  margin: 0 auto;
  height: 150rpx;
  margin-top: 30rpx;
  border: 1px solid #ddd;
  border-radius: 10rpx;
  text-align: center;
  line-height: 150rpx;
}