/* pages/bankList/list.wxss */
.bank{
    background-color: #F7F7F7;
}
.item{
    display: flex;
    flex-direction: column;
    width: 100%;
    background-color: #ffffff;
    margin-top: 20rpx;
}
.item .i_title{
    height: 128rpx;
    line-height: 128rpx;
    color: #333333;
    width: 90%;
    margin: 0 auto;
    font-size: 28rpx;
}
.i_title .phone{
    margin-left: 40rpx;
}
.item .i_content{
    height: 100rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    width: 90%;
    margin: 0 auto;
    font-size: 26rpx;
    color: #999999;
    border-top: 1rpx solid rgba(180,180,180,0.2);
}
.i_content .i_img{
    display: flex;
    align-items: center;
}

.i_img text{
    margin-left: 40rpx;
}

image{
    width:40rpx;
    height: 40rpx;
}
.foot{
    width: 100%;
    height: 90rpx;
    background-color: #FF5D22;
    line-height: 90rpx;
    text-align: center;
    color: #ffffff;
    font-size: 30rpx;
    position: absolute;
    bottom: 0;
    left: 0;
}
