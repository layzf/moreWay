.container{
    position: absolute;
    bottom: 180rpx;
    left: 0;
    width: 100%;
    height: 330rpx;
}
.container .slide{
    height: 400rpx;
    -webkit-border-radius: 10rpx;
    -moz-border-radius: 10rpx;
    border-radius: 10rpx;
    display: flex;
    white-space: nowrap;
}
.s-item{
    width: 450rpx;
    height:400rpx;
    margin-left:30rpx;
    box-shadow:10rpx 10rpx 20rpx rgba(180,180,180,0.3);
}
