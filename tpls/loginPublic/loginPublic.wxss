/* 授权弹出框 */
.model{
  width:100%;
  position:absolute;
  z-index:99;
  top:0;
  background-color:#fff;
  left:0;
}
.model_head{
  width: 500rpx;
  height: 460rpx;
  background: #ffffff;
  margin: 0 auto;
}
.model_head .model_left{
width: 100%;
height: 260rpx;
border-top-left-radius: 10rpx;
border-top-right-radius: 10rpx;
}
.model_head .notice1{
  color:#333333;
  font-size:30rpx;
  width:100%;
  height:120rpx;
  text-align:center;
  margin:80rpx 0;
  display:flex;
  flex-direction:column;
  justify-content:space-between;
}
.model .button_sure{
  width:80%;
  height:100rpx;
  text-align:center;
  line-height:100rpx;
  background-color:#E94816 !important;
  color:#ffffff;
  font-size:32rpx;
  border-bottom:none;
  border-radius:100rpx;
}
.m_img{
  width: 40%;
  height: 80rpx;
  display: block;
  margin: 0 auto;
}
