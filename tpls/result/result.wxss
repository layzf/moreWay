.res-w{
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
  align-items: center;
  justify-content: center; 
  border-top:1px solid #f7f7f7;
  padding:80rpx 0 108rpx;
  background-color: #fff;
}

.res-w .icon-res{
  width:90rpx;
  height:90rpx;
  margin-bottom:24rpx;
}

.res-w .text-res-s{
  color:#666;
  font-size:30rpx;
}
.res-w .text-res-l{
  color:#333;
  font-size:36rpx;
}

.res-con{
  padding:0 28rpx;
  background-color: #fff;
  color:#666;
}

.res-con .head{
  border-bottom:1px solid #f7f7f7;
  line-height:88rpx;
  color:#999;
}

.res-con .con{
  padding-top:20rpx;
}

.res-con .con .p{
  margin-bottom:28rpx;
}

.con-w{
  padding:200rpx 28rpx 0;
  background-color: #fff;
  color:#666;
  height:100%;
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
  align-items: center;
  justify-content: center;
}

.con-w .des{
  font-size:32rpx;
  margin-bottom:50rpx;
}

.con-w button{
  background:rgba(233,72,22,.1);
  color:#E94816;
  border-color:rgba(233,72,22,.5);
  font-size:28rpx;
  line-height:68rpx;
  width:240rpx;
}

.border-w{
  height:20rpx;
  background-color: #F7F7F7;
}

